<tal:block i18n:domain="nive">
<input type="date"
       name="${field.name}"
       value="${cstruct}" 
       tal:attributes="size field.widget.size;
                       class field.widget.css_class"
       id="${field.oid}">
<script type="text/javascript">
  reform.addCallback(
    '${field.oid}',
    function(oid) {
        $('#' + oid).datepicker({dateFormat: 'yy-mm-dd'});
    }
  );
</script>
</tal:block>